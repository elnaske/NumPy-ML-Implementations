   "source": [
    "np.random.seed(42)\n",
    "X = np.random.rand(100,10) * 100\n",
    "\n",
    "rvq = ResidualVQ(10, 8)\n",
    "\n",
    "rvq.fit(X)\n",
    "encoded = rvq.transform(X)\n",
    "X_hat = rvq.decode(encoded)\n",
    "\n",
    "print(np.mean(X - X_hat))"
